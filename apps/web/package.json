{
  "name": "@dkstore/web",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.6.4",
  "description": "DkStore Website.",
  "scripts": {
    "dev": "concurrently --kill-others --names \"next,email,worker\" -c \"black,blue,yellow\" \"pnpm dev:next\" \"pnpm dev:email\" \"pnpm dev:worker\"",
    "dev:next": "next dev --turbo",
    "dev:email": "email dev -p 3001",
    "dev:worker:start": "pm2 start ecosystem.config.cjs --watch --env development && pm2 logs",
    "dev:worker": "concurrently --kill-others --names \"build,logs\" -c \"blue,yellow\" \"rslib build --watch\" \"pnpm dev:worker:start\"",
    "postdev": "pnpm pm2:down",
    "pm2:down": "pm2 delete all",
    "clean": "git clean -xdf .next .turbo node_modules",
    "build": "pnpm build:worker && pnpm build:next",
    "build:next": "next build",
    "build:worker": "rslib build",
    "start": "next start",
    "start:worker": "pm2-runtime start ecosystem.config.cjs --env production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "pnpm db:generate"
  },
  "dependencies": {
    "@dkstore/env": "workspace:*",
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-email/components": "0.0.34",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@uidotdev/usehooks": "^2.4.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.43.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "colorette": "^2.0.20",
    "jose": "^6.0.10",
    "lucide-react": "^0.482.0",
    "next": "15.2.2",
    "next-safe-action": "^7.10.4",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.0",
    "pm2": "^6.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@dkshs/eslint-config": "4.5.0",
    "@dkstore/tsconfig": "workspace:*",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rslib/core": "^0.5.4",
    "@types/node": "^22.13.10",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "postcss": "^8.5.3",
    "prisma": "^6.5.0",
    "react-email": "3.0.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20"
  }
}

{
  "name": "@dkstore/queue-dashboard",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Queue dashboard for DKStore.",
  "scripts": {
    "build": "rslib build",
    "dev:start": "pm2 start ecosystem.config.cjs --watch --env development && pm2 logs queue-dashboard",
    "dev": "concurrently --kill-others --names \"build,logs\" -c \"blue,yellow\" \"rslib build --watch\" \"pnpm dev:start\"",
    "postdev": "pnpm pm2:down",
    "start": "pm2-runtime start ecosystem.config.cjs --env production",
    "pm2:down": "pm2 delete all",
    "clean": "git clean -xdf .turbo dist node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "^6.7.10",
    "@bull-board/fastify": "^6.7.10",
    "@bull-board/ui": "^6.7.10",
    "@dkstore/env": "workspace:*",
    "@dkstore/queue": "workspace:*",
    "fastify": "^5.2.1",
    "pm2": "^6.0.5"
  },
  "devDependencies": {
    "@dkshs/eslint-config": "4.5.0",
    "@dkstore/tsconfig": "workspace:*",
    "@rslib/core": "^0.5.5",
    "@types/node": "^22.13.10",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20"
  }
}

{
  "name": "@dkstore/utils",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Shared utilities for DkStore.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }

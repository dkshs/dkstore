{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "preserve",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "moduleDetection": "force",
    "moduleResolution": "Bundler",
    "plugins": [{ "name": "next" }]
  },
}
